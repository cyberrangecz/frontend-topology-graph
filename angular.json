{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "topology-lib-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "topology-lib:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "topology-lib:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "graph-topology": {
      "root": "projects/graph-topology",
      "sourceRoot": "projects/graph-topology/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/graph-topology/tsconfig.lib.json",
            "project": "projects/graph-topology/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/graph-topology/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/graph-topology/src/test.ts",
            "tsConfig": "projects/graph-topology/tsconfig.spec.json",
            "karmaConfig": "projects/graph-topology/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/graph-topology/tsconfig.lib.json",
              "projects/graph-topology/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "topology-lib"
}

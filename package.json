{
  "name": "@muni-kypo-crp/topology-graph",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl",
    "test": "ng test",
    "lint": "ng lint && npm run prettier-check",
    "e2e": "ng e2e",
    "prettier-check": "prettier --check \"projects/**/*.ts\"",
    "prettier-fix": "prettier --write \"./projects/**/*.ts\"",
    "ci-test": "ng test --browsers ChromeHeadless --watch=false",
    "build-library": "ng build kypo-topology-graph --configuration production",
    "build-example-app": "ng build kypo-topology-graph-example --configuration production",
    "create-package": "cd dist/kypo-topology-graph && npm pack",
    "build-and-pack": "npm install && npm run build-library && npm run create-package",
    "ci-update-version": "cd ./projects/kypo-topology-graph/ && npm version $TAG_VERSION",
    "ci-build-and-pack": "npm ci --cache .npm --prefer-offline && npm run build-library && npm run create-package",
    "ci-publish-package": "cd dist/kypo-topology-graph && npm publish",
    "api": "json-server -w utils/json-server/db.json -r utils/json-server/routes.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-check"
    }
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.14",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/material": "^16.2.14",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@muni-kypo-crp/topology-model": "~16.0.0",
    "@sentinel/auth": "~16.1.1",
    "angular-oauth2-oidc": "^16.0.0",
    "angular-oauth2-oidc-jwks": "^16.0.0",
    "core-js": "^3.23.3",
    "d3": "^7.9.0",
    "moment-mini": "^2.24.0",
    "rxjs": "~7.5.5",
    "tslib": "^2.4.0",
    "typescript-collections": "^1.3.3",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1602.14",
    "@angular-devkit/build-angular": "^16.2.14",
    "@angular-eslint/builder": "~16.3.1",
    "@angular-eslint/eslint-plugin": "^16.3.1",
    "@angular-eslint/eslint-plugin-template": "^16.3.1",
    "@angular-eslint/schematics": "^16.3.1",
    "@angular-eslint/template-parser": "^16.3.1",
    "@angular/cli": "^16.2.14",
    "@angular/compiler-cli": "^16.2.12",
    "@angular/language-service": "^16.2.12",
    "@types/d3": "^7.4.3",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "husky": "^8.0.1",
    "jasmine-core": "~4.2.0",
    "jasmine-spec-reporter": "~7.0.0",
    "json-server": "^0.17.4",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ng-packagr": "^16.2.3",
    "prettier": "^2.7.1",
    "protractor": "~7.0.0",
    "ts-node": "~10.8.2",
    "typescript": "~4.9.5"
  }
}
